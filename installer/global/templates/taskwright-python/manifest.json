  "placeholders": {
    "ProjectName": {
      "name": "{{ProjectName}}",
      "description": "Project name in PascalCase (e.g., TaskManager, CodeAnalyzer)",
      "required": true,
      "pattern": "^[A-Z][A-Za-z0-9]*$",
      "example": "TaskManager"
