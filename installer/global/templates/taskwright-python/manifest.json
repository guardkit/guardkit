      "example": "task-manager"
    },
    "ProjectDescription": {
      "name": "{{ProjectDescription}}",
      "description": "Brief description of the CLI tool",
      "required": true,
      "example": "A command-line task management tool"
    },
    "AuthorName": {
      "name": "{{AuthorName}}",
      "description": "Author or organization name",
      "required": false,
      "default": "Your Name",
      "example": "Jane Developer"
    },
    "AgentName": {
      "name": "{{AgentName}}",
